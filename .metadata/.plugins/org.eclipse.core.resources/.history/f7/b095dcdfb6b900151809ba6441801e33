
public class cNoeuds {
	private cFormes M_Container = null;
	private cNoeuds M_Suivant = null;
	
	public cNoeuds( cFormes _formes){
		if( _formes != null ){
			M_Container = _formes;
			M_Suivant = null;
		}
	}
	public void setSuivant( cNoeuds _noeud ){
		if( _noeud != null ){
			M_Suivant = _noeud;
		}
		else{
			//
		}
	}
	public cNoeuds getSuivant(){
		return M_Suivant;
	}
	public cFormes getForme(){
		return M_Container;
	}
}
